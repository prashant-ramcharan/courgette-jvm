    <link href="https://cdn.jsdelivr.net/gh/prashant-ramcharan/courgette-jvm/src/main/resources/report/css/chartjs.min.css" rel="stylesheet">
    <link href="https://cdn.jsdelivr.net/gh/prashant-ramcharan/courgette-jvm/src/main/resources/report/css/report.min.css" rel="stylesheet">
    <link href="https://cdn.jsdelivr.net/gh/prashant-ramcharan/courgette-jvm/src/main/resources/report/css/dataTables.bootstrap4.min.css" rel="stylesheet">
</head>
<body class="fixed-nav sticky-footer bg-dark" id="page-top">
<!-- Navigation -->
<nav class="navbar navbar-expand-lg navbar-dark bg-dark" id="mainNav">
    <li class="navbar-brand" href="#" style="padding-left: 10px;">{{reportTitle}}</li>
</nav>
<div class="content-wrapper">
    <div class="container-fluid">
        <!-- Result Cards -->
        <div class="row">
            <div class="col-lg-3 col-sm-6 mb-3">
                <div class="card text-white bg-primary o-hidden h-100">
                    <div class="card-body">
                        <div class="card-body-icon">
                            <i class="fa fa-fw"></i>
                        </div>
                        <div class="mr-10">
                            Total {{label}}
                        </div>
                    </div>
                    <div class="card-footer text-white clearfix small z-1" style="cursor: pointer;"
                         onclick="setSearch('');">
                        <span class="float-left text-white" id="card-total">{{total}}</span>
                    </div>
                </div>
            </div>
            <div class="col-lg-3 col-sm-6 mb-3">
                <div class="card text-white bg-success o-hidden h-100">
                    <div class="card-body">
                        <div class="card-body-icon">
                            <i class="fa fa-fw"></i>
                        </div>
                        <div class="mr-10">
                            Passed {{label}}
                        </div>
                    </div>
                    <div class="card-footer text-white clearfix small z-1" style="cursor: pointer;"
                         onclick="setSearch('Passed');">
                        <span class="float-left text-white" id="card-passed">{{passed}}</span>
                    </div>
                </div>
            </div>
            <div class="col-lg-3 col-sm-6 mb-3">
                <div class="card text-white bg-danger o-hidden h-100">
                    <div class="card-body">
                        <div class="card-body-icon">
                            <i class="fa fa-fw"></i>
                        </div>
                        <div class="mr-10">
                            Failed {{label}}
                        </div>
                    </div>
                    <div class="card-footer text-white clearfix small z-1" style="cursor: pointer;"
                         onclick="setSearch('Failed');">
                        <span class="float-left text-white" id="card-failed">{{failed}}</span>
                    </div>
                </div>
            </div>
            <div class="col-lg-3 col-sm-6 mb-3">
                <div class="card text-white bg-warning o-hidden h-100">
                    <div class="card-body">
                        <div class="card-body-icon">
                            <i class="fa fa-fw"></i>
                        </div>
                        <div class="mr-10">
                            Re-run {{label}}
                        </div>
                    </div>
                    <div class="card-footer text-white clearfix small z-1" style="cursor: pointer;"
                         onclick="setSearch('Rerun');">
                        <span class="float-left" id="card-rerun">{{rerun}}</span>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-5">
                <div class="card mb-3">
                    <div class="card-header">
                        <i class="fa"></i>Run Information
                    </div>
                    <div class="card-body">
                        <div class="row">
                            <div class="col-sm-4">
                                Timestamp:
                            </div>
                            <div class="col-sm-8">{{timestamp}}</div>
                        </div>
                        <div class="row mt-3">
                            <div class="col-sm-4">
                                Duration:
                            </div>
                            <div class="col-sm-8">{{duration}}</div>
                        </div>
                        <div class="row mt-3">
                            <div class="col-sm-4">
                                Parallel Threads:
                            </div>
                            <div class="col-sm-8">{{threads}}</div>
                        </div>
                        <div class="row mt-3">
                            <div class="col-sm-4">
                                Parallel Run Level:
                            </div>
                            <div class="col-sm-8">{{run_level}}</div>
                        </div>
                        <div class="row mt-3">
                            <div class="col-sm-4">
                                Cucumber Tags:
                            </div>
                            <div class="col-sm-8" style="overflow-wrap:break-word;">{{tags}}</div>
                        </div>
                        <div class="row mt-3">
                            <div class="col-sm-4">
                                Cucumber Features:
                            </div>
                            <div class="col-sm-8" style="overflow-wrap:break-word;">{{features}}</div>
                        </div>
                        <div class="row mt-3">
                            <div class="col-sm-4">
                                Cucumber Report:
                            </div>
                            <div class="col-sm-8"><a target="_blank" href="{{cucumber_report}}">View published
                                report</a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-sm-3">
                <div class="card mb-3">
                    <div class="card-header">
                        <i class="fa"></i>Environment Information
                    </div>
                    <div class="card-body">
                        <div class="row">
                            <div class="col-sm-7">
                                OS Name:
                            </div>
                            <div>{{os_name}}</div>
                        </div>
                        <div class="row mt-4">
                            <div class="col-sm-7">
                                OS Architecture:
                            </div>
                            <div>{{os_arch}}</div>
                        </div>
                        <div class="row mt-4">
                            <div class="col-sm-7">
                                Java Version:
                            </div>
                            <div>{{java_version}}</div>
                        </div>
                        <div class="row mt-4">
                            <div class="col-sm-7">
                                Cucumber Version:
                            </div>
                            <div>7.11.0</div>
                        </div>
                        <div class="row mt-4">
                            <div class="col-sm-7">
                                Courgette Version:
                            </div>
                            <div>6.9.0</div>
                        </div>
                        <div class="row mt-4">
                            <div><a href class="col-sm-9" data-toggle="modal" data-target="#environment_info">Additional
                                environment information</a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-lg-4">
                <div class="card">
                    <div class="mt-3 mb-3">
                        <canvas id="pieChart" width="100%" height="321"></canvas>
                    </div>
                </div>
            </div>
        </div>
        <!-- Result Table -->
        <div class="card mb-3">
            <div class="card-header">
                <i class="fa fa-table"></i>
                Results
            </div>
            <div class="card-body">
                <div class="table-responsive">
                    <table class="table table-bordered" width="100%" id="dataTable" cellspacing="0">
                        <thead>
                        <tr>
                            <th width="95%">Feature</th>
                            <th>Result</th>
                        </tr>
                        </thead>
                        <tbody>
                        {{#results}}
                            {{{.}}}
                        {{/results}}
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
    <!-- /.container-fluid -->
    <!-- Modals -->
    {{#modals}}
        {{{.}}}
    {{/modals}}
</div>
<!-- /.content-wrapper -->

<script src="https://cdn.jsdelivr.net/gh/prashant-ramcharan/courgette-jvm/src/main/resources/report/js/jquery-3.5.1.slim.min.js"></script>
<script src="https://cdn.jsdelivr.net/gh/prashant-ramcharan/courgette-jvm/src/main/resources/report/js/bootstrap.bundle.min.js"></script>
<script src="https://cdn.jsdelivr.net/gh/prashant-ramcharan/courgette-jvm/src/main/resources/report/js/jquery.dataTables.min.js"></script>
<script src="https://cdn.jsdelivr.net/gh/prashant-ramcharan/courgette-jvm/src/main/resources/report/js/dataTables.bootstrap4.min.js"></script>
<script src="https://cdn.jsdelivr.net/gh/prashant-ramcharan/courgette-jvm/src/main/resources/report/js/chart.min.js"></script>

<script>
      var passed=$("#card-passed").text(),failed=$("#card-failed").text(),rerun=$("#card-rerun").text(),ctxP=document.getElementById("pieChart").getContext("2d"),pieChart=new Chart(ctxP,{type:"pie",data:{labels:["Passed","Failed","Re-run"],datasets:[{data:[passed,failed,rerun],backgroundColor:["#28a745","#dc3545","#ffbb33"]}]},options:{responsive:!0,maintainAspectRatio:!1}});
</script>

<script>
   function setSearch(e){var t=e;e.length<2&&(t=""),$("input[type='search']").val(t).trigger($.Event("keyup",{keyCode:13}))}
</script>

<script>
    $(document).ready(function() {
      $('#dataTable').DataTable();
    });
</script>
</div>
</body>
</html>